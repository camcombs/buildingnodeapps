{"ts":1370537113939,"silentsave":true,"restoring":false,"patch":[[{"diffs":[[1,"<!-- example3_3 backbone -->\n\n<html>\n    <head>\n        <script type=\"text/javascript\" src=\"http://cdnjs.cloudflare.com/ajax/libs/jquery/1.7.2/jquery.min.js\"></script>\n        <script type=\"text/javascript\" src=\"http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.3.3/underscore-min.js\" ></script>\n        <script type=\"text/javascript\" src=\"http://cdnjs.cloudflare.com/ajax/libs/backbone.js/0.9.2/backbone-min.js\"></script>\n    </head>\n    <body>\n        <div id=\"certificate\"></div>\n        <script type=\"text/template\" id=\"tpl-certificate\">\n            <h1><%= name %></h1>\n            <p>DOB: <%= dob %></p>\n        </script>\n        <script type=\"text/javascript\">\n            CertificateView = Backbone.View.extend({\n                template:_.template($('#tpl-certificate').html()),\n                <!-- that underscore is to call Underscore template function -->\n                \n                initialize: function() {\n                    this.render();\n                },\n                    \n                render: function() {\n                    var templateArgs = {\n                        name: \"Guy Incognito\", dob: \"March 14, 1967\"\n                    };\n                $(this.el).html(this.template(templateArgs));\n                }\n            });\n            \n            var certificate_view = new CertificateView({ el: $(\"#certificate\")});\n            \n        </script>\n\n    </body>\n</html>"]],"start1":0,"start2":0,"length1":0,"length2":1418}]],"length":1418}
